include "lib/wasi.fire"
include "lib/std.fire"

double_str struct :
  first  str
  second str
end

print_double inline: double_str : println println end

hello double_str := "Hello World!" "Other Text" end
new_text str := "-> a Really long text" end

_start export::
  hello print_double
  
  *new_text trim-left
  *new_text trim-left
  *new_text trim-left
  *new_text trim-left
  *new_text trim-left

  new_text "Another one" !hello
  hello print_double

  *hello .*second .*len dup dec32 dec32
  *hello.second.len     dup dec32 dec32

  *hello .*first .len
  hello.first.data 
  hello.second print_double
end
